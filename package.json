{
  "name": "jiral",
  "private": true,
  "displayName": "Jiral",
  "description": "Make Jira issue operations easier.",
  "version": "0.0.0",
  "engines": {
    "vscode": "^1.59.0"
  },
  "author": "thilllon",
  "main": "./out/extension.js",
  "categories": [],
  "keywords": [
    "jira",
    "issues",
    "bugs",
    "list",
    "tracking"
  ],
  "icon": "assets/jira-logo.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/thilllon/vscode-jiral"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -watch",
    "publish": "pnpm build && vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint --fix .",
    "test": "pnpm build && node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/copy-paste": "^1.1.33",
    "@types/glob": "^8.1.0",
    "@types/jira-client": "^7.1.9",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.7",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@vscode/test-electron": "^2.3.8",
    "@vscode/vsce": "^2.22.0",
    "eslint": "^8.56.0",
    "glob": "^10.3.10",
    "mocha": "^10.2.0",
    "semantic-release": "^22.0.12",
    "typescript": "^5.3.3"
  },
  "activationEvents": [
    "onView:jiraIssues"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "jira-issue",
          "title": "jira-issue",
          "icon": "./assets/jira-logo.png"
        }
      ]
    },
    "views": {
      "jira-issue": [
        {
          "id": "jira-issue",
          "name": "jira-issue"
        }
      ]
    },
    "commands": [
      {
        "command": "jira-issue.bearer",
        "title": "set your jira bearer"
      },
      {
        "command": "jira-issue.baseUrl",
        "title": "set your jira base url"
      },
      {
        "command": "jira-issue.username",
        "title": "set your jira username"
      },
      {
        "command": "jira-issue.password",
        "title": "set your jira password"
      },
      {
        "command": "jira-issue.refresh",
        "title": "Refresh",
        "icon": {
          "light": "./assets/light/refresh.svg",
          "dark": "./assets/dark/refresh.svg"
        }
      },
      {
        "command": "jira-issue.filter",
        "title": "Filter",
        "icon": {
          "light": "./assets/light/filter.svg",
          "dark": "./assets/dark/filter.svg"
        }
      },
      {
        "command": "jira-issue.assignToMe",
        "title": "Assign To Me",
        "icon": {
          "light": "./assets/light/assignToMe.svg",
          "dark": "./assets/dark/assignToMe.svg"
        }
      },
      {
        "command": "jira-issue.check",
        "title": "Commit By Issue",
        "icon": {
          "light": "./assets/light/check.svg",
          "dark": "./assets/dark/check.svg"
        }
      },
      {
        "command": "jira-issue.openIssue",
        "title": "Open Issue"
      },
      {
        "command": "jira-issue.changeStatus",
        "title": "Change Issue Status"
      },
      {
        "command": "jira-issue.gitCommit",
        "title": "Commit By Issue"
      },
      {
        "command": "jira-issue.copyJiraIssue",
        "title": "Copy Issue Number",
        "icon": {
          "light": "./assets/light/copy.svg",
          "dark": "./assets/dark/copy.svg"
        }
      },
      {
        "command": "jira-issue.copyJiraIssueTitle",
        "title": "Copy Issue Title",
        "icon": {
          "light": "./assets/light/clippy.svg",
          "dark": "./assets/dark/clippy.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "jira-issue.refresh",
          "when": "view == jira-issue",
          "group": "navigation"
        },
        {
          "command": "jira-issue.filter",
          "when": "view == jira-issue",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "jira-issue.openIssue",
          "when": "view == jira-issue && viewItem == issue",
          "group": "1_navigation"
        },
        {
          "command": "jira-issue.changeStatus",
          "when": "view == jira-issue && viewItem == issue",
          "group": "1_navigation"
        },
        {
          "command": "jira-issue.gitCommit",
          "when": "view == jira-issue && viewItem == issue",
          "group": "9_cutcopypaste"
        },
        {
          "command": "jira-issue.copyJiraIssue",
          "when": "view == jira-issue && viewItem == issue",
          "group": "9_cutcopypaste"
        },
        {
          "command": "jira-issue.copyJiraIssueTitle",
          "when": "view == jira-issue && viewItem == issue",
          "group": "9_cutcopypaste"
        },
        {
          "command": "jira-issue.copyJiraIssue",
          "when": "view == jira-issue  && viewItem == issue",
          "group": "inline"
        },
        {
          "command": "jira-issue.copyJiraIssueTitle",
          "when": "view == jira-issue  && viewItem == issue",
          "group": "inline"
        },
        {
          "command": "jira-issue.assignToMe",
          "when": "view == jira-issue  && viewItem == issue",
          "group": "inline"
        },
        {
          "command": "jira-issue.check",
          "when": "view == jira-issue  && viewItem == issue",
          "group": "inline"
        },
        {
          "command": "jira-issue.assignToMe",
          "when": "view == jira-issue  && viewItem == issue",
          "group": "assignToMe@2"
        }
      ]
    },
    "keybindings": [
      {
        "command": "jira-issue.bearer",
        "key": "Cmd+Shift+[",
        "mac": "Cmd+Shift+[",
        "win": "Ctrl+Shift+["
      },
      {
        "command": "jira-issue.baseUrl",
        "key": "Cmd+Shift+]",
        "mac": "Cmd+Shift+]",
        "win": "Ctrl+Shift+]"
      },
      {
        "command": "jira-issue.username",
        "key": "Cmd+Shift+-",
        "mac": "Cmd+Shift+-",
        "win": "Ctrl+Shift+-"
      },
      {
        "command": "jira-issue.password",
        "key": "Cmd+Shift+\\",
        "mac": "Cmd+Shift+\\",
        "win": "Ctrl+Shift+\\"
      }
    ],
    "configuration": {
      "title": "jira-issue Configuration",
      "properties": {
        "jira-issue.bearer": {
          "type": "string",
          "default": "",
          "description": "Your jira bearer token"
        },
        "jira-issue.username": {
          "type": "string",
          "default": "",
          "description": "Your jira username"
        },
        "jira-issue.password": {
          "type": "string",
          "default": "",
          "description": "Your jira password"
        },
        "jira-issue.baseUrl": {
          "type": "string",
          "default": "",
          "description": "Your jira base url"
        },
        "jira-issue.replaceMethods": {
          "type": "array",
          "default": [
            [
              "halo jira-issue",
              "halo jira-issue"
            ]
          ],
          "description": "Your jira commit replace methods, e.g. [regexp string, string]"
        }
      }
    }
  },
  "dependencies": {
    "copy-paste": "^1.5.3",
    "jira-client": "^8.2.2"
  }
}
